		pins_cmd1_dat {
			pins = <PINMUX_GPIO170__FUNC_GPIO170>;
		};
	};

	aud_pins_smartpa_on: aud_pins_smartpa_on {
		pins_cmd0_dat {
			pins = <PINMUX_GPIO174__FUNC_I2S0_DI>;
